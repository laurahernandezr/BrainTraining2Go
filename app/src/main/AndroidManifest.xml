<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.sjsu.android.cs175finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CS175FinalProject">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ChooseGameActivity"/>
        <activity android:name=".GameDescriptionActivity"/>
        <activity android:name=".GameInstructionsActivity"/>
        <activity android:name=".MultitaskingActivity">
        </activity>
        <activity android:name=".MultitaskingResults">
        </activity>
        <activity android:name=".SearchActivity"/>
        <activity android:name=".SearchResults"/>
        <activity android:name=".SignInActivity"/>
        <activity android:name=".SignUpActivity"/>

        <activity android:name=".CardMatchActivity"/>
        <activity android:name=".CardsResults"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>